subsection %%\hyperref[parameters:Material_20model]{Material model}%
  set %%\hyperref[parameters:Material model/Model name]{Model name}% = latent heat %% \index[prmindex]{Model name} \index[prmindexfull]{Material model!Model name} %
  subsection %%\hyperref[parameters:Material_20model/Latent_20heat]{Latent heat}%

    # The change of density across the phase transition. Together with the
    # Clapeyron slope, this is what determines the entropy change.
    set %%\hyperref[parameters:Material model/Latent heat/Phase transition density jumps]{Phase transition density jumps}%                 = 115.6 %% \index[prmindex]{Phase transition density jumps} \index[prmindexfull]{Material model!Latent heat!Phase transition density jumps} %
    set %%\hyperref[parameters:Material model/Latent heat/Corresponding phase for density jump]{Corresponding phase for density jump}%           = 0 %% \index[prmindex]{Corresponding phase for density jump} \index[prmindexfull]{Material model!Latent heat!Corresponding phase for density jump} %

    # If the temperature is equal to the phase transition temperature, the 
    # phase transition will occur at the phase transition depth. However, 
    # if the temperature deviates from this value, the Clapeyron slope 
    # determines how much the pressure (and depth) of the phase boundary
    # changes. Here, the phase transition will be in the middle of the box
    # for T=T1. 
    set %%\hyperref[parameters:Material model/Latent heat/Phase transition depths]{Phase transition depths}%                        = 500000 %% \index[prmindex]{Phase transition depths} \index[prmindexfull]{Material model!Latent heat!Phase transition depths} %
    set %%\hyperref[parameters:Material model/Latent heat/Phase transition temperatures]{Phase transition temperatures}%                  = 1000 %% \index[prmindex]{Phase transition temperatures} \index[prmindexfull]{Material model!Latent heat!Phase transition temperatures} %
    set %%\hyperref[parameters:Material model/Latent heat/Phase transition Clapeyron slopes]{Phase transition Clapeyron slopes}%              = 1e7 %% \index[prmindex]{Phase transition Clapeyron slopes} \index[prmindexfull]{Material model!Latent heat!Phase transition Clapeyron slopes} %

    # We set the width of the phase transition to 5 km. You may want to 
    # change this parameter to see how latent heating depends on the width
    # of the phase transition. 
    set %%\hyperref[parameters:Material model/Latent heat/Phase transition widths]{Phase transition widths}%                        = 5000 %% \index[prmindex]{Phase transition widths} \index[prmindexfull]{Material model!Latent heat!Phase transition widths} %

    set %%\hyperref[parameters:Material model/Latent heat/Reference density]{Reference density}%                              = 3400 %% \index[prmindex]{Reference density} \index[prmindexfull]{Material model!Latent heat!Reference density} %
    set %%\hyperref[parameters:Material model/Latent heat/Reference specific heat]{Reference specific heat}%                        = 1000 %% \index[prmindex]{Reference specific heat} \index[prmindexfull]{Material model!Latent heat!Reference specific heat} %
    set %%\hyperref[parameters:Material model/Latent heat/Reference temperature]{Reference temperature}%                          = 1000 %% \index[prmindex]{Reference temperature} \index[prmindexfull]{Material model!Latent heat!Reference temperature} %
    set %%\hyperref[parameters:Material model/Latent heat/Thermal conductivity]{Thermal conductivity}%                           = 2.38 %% \index[prmindex]{Thermal conductivity} \index[prmindexfull]{Material model!Latent heat!Thermal conductivity} %

    # We set the thermal expansion amd the compressibility to zero, so that 
    # all temperature (and density) changes are caused by advection, diffusion 
    # and latent heating. 
    set %%\hyperref[parameters:Material model/Latent heat/Thermal expansion coefficient]{Thermal expansion coefficient}%                  = 0.0 %% \index[prmindex]{Thermal expansion coefficient} \index[prmindexfull]{Material model!Latent heat!Thermal expansion coefficient} %
    set %%\hyperref[parameters:Material model/Latent heat/Compressibility]{Compressibility}%                                = 0.0 %% \index[prmindex]{Compressibility} \index[prmindexfull]{Material model!Latent heat!Compressibility} %

    # Viscosity is constant. 
    set %%\hyperref[parameters:Material model/Latent heat/Thermal viscosity exponent]{Thermal viscosity exponent}%                     = 0.0 %% \index[prmindex]{Thermal viscosity exponent} \index[prmindexfull]{Material model!Latent heat!Thermal viscosity exponent} %
    set %%\hyperref[parameters:Material model/Latent heat/Viscosity]{Viscosity}%                                      = 8.44e21 %% \index[prmindex]{Viscosity} \index[prmindexfull]{Material model!Latent heat!Viscosity} %
    set %%\hyperref[parameters:Material model/Latent heat/Viscosity prefactors]{Viscosity prefactors}%                           = 1.0, 1.0 %% \index[prmindex]{Viscosity prefactors} \index[prmindexfull]{Material model!Latent heat!Viscosity prefactors} %
    set %%\hyperref[parameters:Material model/Latent heat/Composition viscosity prefactor]{Composition viscosity prefactor}%                = 1.0 %% \index[prmindex]{Composition viscosity prefactor} \index[prmindexfull]{Material model!Latent heat!Composition viscosity prefactor} %
  end
end
